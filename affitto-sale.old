---
layout: default
title: titles.affitto-sale
id: affitto-sale
namespace: affitto-sale
last_modified_at: 2019-06-11T20:00:00+02:00
permalink: /affitto-sale/
permalink_en: /rent-location/
group:
    id: servizi
    title: titles.servizi
features:
    breadcrumb: true
    contacts: true
---

<section class="whiteSection afterWhiteSection full-width clearfix">
    <div class="container">
        <div class="row">

            <div class="col-sm-6 col-xs-12">
                <div class="schoolInfo">
                    <h2>{% t page.title %}</h2>
                    <div class="text-justify">

                        <p>
                            {% t rent.p1 %}
                        </p>
                        <p>
                            {% t rent.p2 %}
                        </p>
                        <ul class="list-unstyled para-list">
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i1 %}</strong></li>
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i2 %}</strong></li>
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i3 %}</strong></li>
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i4 %}</strong></li>
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i5 %}</strong></li>
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i6 %}</strong></li>
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i7 %}</strong></li>
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i8 %}</strong></li>
                            <li><i class="fa fa-check" aria-hidden="true"></i> <strong>{% t rent.items.i9 %}</strong></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-sm-6 col-xs-12">
                <img src="{{ site.baseurl_root }}/img/main/giardino.jpg" alt="giardino" class="img-responsive img-rounded">
            </div>
        </div>
        <div class="row space-medium">
            <div class="col-xs-12 col-md-12">
                <div class="row isotopeContainer">

                    {% for image in site.static_files %}
                    {% if image.path contains 'assets/home_gallery/m/aule' %}
                    <div class="col-md-3 col-sm-6 col-xs-12 isotopeSelector {{ class }}">
                        <article class="">
                            <figure>
                                <img src="{{ site.baseurl }}{{ image.path }}" alt="image" class="img-rounded" />
                                <div class="overlay-background">
                                    <div class="inner"></div>
                                </div>
                                <div class="overlay">
                                    <a class="fancybox-pop" rel="portfolio-1" href="{{ site.baseurl }}{{ image.path }}">
                                        <i class="fa fa-search-plus" aria-hidden="true"></i>
                                    </a>
                                </div>
                            </figure>
                        </article>
                    </div>
                    {% endif %}
                    {% endfor %}
                </div>
            </div>
        </div>
        <div class="row space-medium">
            <p>
                {% t rent.p3 %}
            </p>
        </div>
        <div class="row space-medium">
            <div class="col-xs-12 col-md-6">
                <p style="text-align: center">
                    <a href="{% tl contatti %}" class="btn btn-primary"><i class="fa fa-pencil-square-o" aria-hidden="true"></i> {% t global.contact_us %}</a>
                </p>
            </div>
            <div class="col-xs-12 col-md-6">
                <p style="text-align: center">
                    <a href="{% tl galleria %}" class="btn btn-primary"><i class="fa fa-image"></i> {% t global.full_gallery %}</a>
                </p>
            </div>
        </div>
    </div>
</section>
